{
  "name": "electron-vite-boilerplate-vuetify",
  "version": "1.0.0",
  "description": "A boilerplate Electron application with Vuetify",
  "main": "./out/main/index.js",
  "author": "DeezjaVu",
  "homepage": "https://github.com/DeezjaVu/electron-vite-boilerplate-vuetify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeezjaVu/electron-vite-boilerplate-vuetify.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "vite:dev": "electron-vite dev",
    "vite:build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run vite:build && electron-builder --dir",
    "build:win": "npm run vite:build && electron-builder --win",
    "build:mac": "npm run vite:build && electron-builder --mac",
    "build:linux": "npm run vite:build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.11.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "pinia": "^3.0.1",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.1",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.2.4",
    "vite-plugin-vuetify": "^2.1.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.1",
    "vuetify": "^3.8.0"
  }
}
