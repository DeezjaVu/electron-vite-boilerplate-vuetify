{
  "name": "electron-vite-boilerplate-vuetify",
  "version": "1.0.0",
  "description": "A boilerplate Electron application with Vuetify",
  "main": "./out/main/index.js",
  "author": "DeezjaVu",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "vite:dev": "electron-vite dev",
    "vite:build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run vite:build && electron-builder --dir",
    "build:win": "npm run vite:build && electron-builder --win",
    "build:mac": "npm run vite:build && electron-builder --mac",
    "build:linux": "npm run vite:build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "pinia": "^2.3.1",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.4",
    "unplugin-fonts": "^1.3.1",
    "vite": "^5.4.14",
    "vite-plugin-vuetify": "^2.1.0",
    "vue": "^3.5.13",
    "vuetify": "^3.7.11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "vue-demi"
    ]
  }
}
